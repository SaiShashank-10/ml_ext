## Task 2b  Binarizer-----
import numpy
import pandas as pd
from sklearn.preprocessing import Binarizer

names  = ['preg', 'plass', 'press', 'skin', 'test', 'mass','pedi', 'age', 'class']
dataframe = pd.read_csv('/content/diabetes (1).csv', names = names)

array = dataframe.values
print(array)

X = array[:, 0:8]
Y = array[:, 8]

binarizer  = Binarizer(threshold = 0.0).fit(X)
binaryX = binarizer.transform(X)

numpy.set_printoptions(precision = 3)
print(binaryX[0:5, :])
